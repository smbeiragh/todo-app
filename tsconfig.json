{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "lib": ["es5", "es2017", "dom"],
    "moduleResolution": "node"
  },
  "exclude": [
    "node_modules"
  ]
}
